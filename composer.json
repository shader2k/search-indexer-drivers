{
    "name": "shader2k/search-indexer-drivers",
    "description": "Drivers for shader2k/search-indexer package",
    "type": "library",
    "keywords": [
        "search-indexer",
        "search-indexer driver",
        "elasticsearch",
        "elasticsearch driver"
    ],
    "authors": [
        {
            "name": "shader2k",
            "email": "shader2k@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
    },
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "shader2k/search-indexer": "^1",
        "elasticsearch/elasticsearch": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "files": [
            "vendor/shader2k/search-indexer/src/Helpers/helpers.php"
        ],
        "psr-4": {
            "Shader2k\\SearchIndexerDrivers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shader2k\\SearchIndexerDrivers\\Tests\\": "tests/",
            "Shader2k\\SearchIndexer\\Tests\\": "vendor/shader2k/search-indexer/tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
